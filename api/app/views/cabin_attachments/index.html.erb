<p id="notice"><%= notice %></p>

<h1>Cabin Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Cabin</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cabin_attachments.each do |cabin_attachment| %>
      <tr>
        <td><%= cabin_attachment.cabin_id %></td>
        <td><%= cabin_attachment.image %></td>
        <td><%= link_to 'Show', cabin_attachment %></td>
        <td><%= link_to 'Edit', edit_cabin_attachment_path(cabin_attachment) %></td>
        <td><%= link_to 'Destroy', cabin_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cabin Attachment', new_cabin_attachment_path %>
